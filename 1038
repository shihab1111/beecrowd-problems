#include<stdio.h>
int main(){
   int a,b;
   float e;
   scanf("%d%d",&a,&b);
   if(a==1){
    e=b*4.00;
    printf("Total: R$ %.2f\n",e);
   }
    if(a==2){
    e=b*4.50;
    printf("Total: R$ %.2f\n",e);
   }
    if(a==3){
    e=b*5.00;
    printf("Total: R$ %.2f\n",e);
   }
    if(a==4){
    e=b*2.00;
    printf("Total: R$ %.2f\n",e);
   }
    if(a==5){
    e=b*1.50;
    printf("Total: R$ %.2f\n",e);
   }


return 0;
}
