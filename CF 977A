#include <stdio.h>
 
int main() {
 
        int a,i,b;
        scanf("%d %d",&a,&b);
        for(i=1;i<=b;i++)
        {
            if(a%10!=0)
            {
                a=a-1;
            }
            else if(a%10==0)
            {
                a=a/10;
            }
        }
 printf("%d\n",a);
    return 0;
}
