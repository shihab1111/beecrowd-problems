#include <stdio.h>
int main()
{

int a,i,j,b;
scanf("%d",&a);
for(i=0;i<a;i++){
    scanf("%d",&b);
    if(b==0)printf("NULL\n");
else if(b%2==0&&b>0)printf("EVEN POSITIVE\n");
else if(b%2==0&&b<0)printf("EVEN NEGATIVE\n");
else if(b%2!=0&&b>0)printf("ODD POSITIVE\n");
else if(b%2!=0&&b<0)printf("ODD NEGATIVE\n");}
    return 0;
}
