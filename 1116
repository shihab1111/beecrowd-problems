#include <stdio.h>

int main()
{
int a,b,c,i;
double e,f,g;
scanf("%d",&a);
for(i=1;i<=a;i++)
{
    scanf("%d %d",&b,&c);

    if(c==0)
    {
        printf("divisao impossivel\n");
    }
    else if(b==0)
    {
        printf("0.0\n");
    }
    else {

   f=b;
   g=c;
  e=f/g;
        printf("%.1lf\n",e);
    }
}
 return 0;
}
